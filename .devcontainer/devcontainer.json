{
  "name": "@cewald/cv",
  "image": "mcr.microsoft.com/vscode/devcontainers/typescript-node:18",
  "customizations": {
    "vscode": {
      "extensions": [
        "bradlc.vscode-tailwindcss",
        "csstools.postcss",
        "dbaeumer.vscode-eslint",
        "Vue.volar",
        "Vue.vscode-typescript-vue-plugin"
      ]
    }
  },
  "forwardPorts": [
    3000,
    4173
  ],
  "portsAttributes": {
    "3000": {
      "label": "dev",
      "protocol": "https",
      "elevateIfNeeded": false
    },
    "4173": {
      "label": "preview",
      "protocol": "https",
      "elevateIfNeeded": false
    }
  }
}
